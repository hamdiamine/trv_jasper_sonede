	<queryString>
		<![CDATA[select
  sag.sag_refe,
  spt.spt_refe,
  equ.equ_realnumber,
  rou.rou_code,
  spt.spt_routeorder,
  par.par_fname||' '||par.par_lname par_shortname
from   plgrel          plg ,
       ligne_plgrel    lpg,
       tecservicepoint spt,
       agrserviceagr   sag,
       tecequipment    equ,
       techequipment   heq,
       agrcustomeragr  cag,
       genparty        par,
       tecroute        rou
where  plg.plgrel_id = $P{plgrel_id}
and  lpg.plgrel_id = plg.plgrel_id
and  rou.rou_id = lpg.rou_id
and  spt.rou_id = rou.rou_id
and  spt.spt_id = sag.spt_id
and  sag.cag_id = cag.cag_id
and  spt.spt_id = heq.spt_id
and  heq.equ_id = equ.equ_id
and  par.par_id = cag.par_id
and  sag.sag_enddt is null
 and heq.heq_enddt is null
 and not exists (select 1
                  from  tecmtrread mrd
                  where mrd.mrd_year = plg.plgrel_year
                  and   mrd.mrd_multicad = decode(nvl(plg.plgrel_trim,0),0,plg.plgrel_mois,plg.plgrel_trim)
                  and   mrd.spt_id = spt.spt_id)]]>
