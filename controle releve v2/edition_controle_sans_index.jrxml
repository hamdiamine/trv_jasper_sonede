	<parameter name="plgrel_id" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select *
from
(
select sag_refe,
       spt_refe,
       rou_code,
       spt.spt_routeorder,
       equ.equ_realnumber,
       (select mme_value from tecmtrmeasure  where mrd_id = mrd.mrd_previous_id and meu_id = 5) aindex, --  A revoir
       par.par_fname ||' '|| par.par_lname par_shortname,
       nvl(vow1.vow_name,nvl(vow2.vow_name,vow3.vow_name)) commentaire
from   plgrel          plg ,
       ligne_plgrel    lpg,
       tecmtrread      mrd,
       tecservicepoint spt,
       agrserviceagr   sag,
       tecequipment    equ,
       agrcustomeragr  cag,
       genparty        par,
       tecroute        rou,
       genvocword vow1,
       genvocword vow2,
       genvocword vow3
where  plg.plgrel_id = $P{plgrel_id}
and    plg.plgrel_year = mrd.mrd_year
and    mrd.mrd_multicad = decode(nvl(plg.plgrel_trim,0),0,plg.plgrel_mois,plg.plgrel_trim)
and    lpg.plgrel_id = plg.plgrel_id
and    lpg.rou_id =  spt.rou_id
and    mrd.spt_id = spt.spt_id
and    sag.spt_id = spt.spt_id
and    mrd.equ_id = equ.equ_id
and    sag.cag_id = cag.cag_id
and    cag.par_id = par.par_id
and    spt.rou_id = rou.rou_id
and    mrd.vow_comm1 = vow1.vow_id(+)
and    mrd.vow_comm2 = vow2.vow_id(+)
and    mrd.vow_comm3 = vow3.vow_id(+)
and not exists(select 1 from tecmtrmeasure where mrd_id = mrd.mrd_id)
)
where commentaire<>'PAS DE CTR'
order by rou_code, spt_routeorder]]>
	</queryString>
